  <rd:ReportID>533fb180-4584-4845-af64-7519b299688f</rd:ReportID>
  <LeftMargin>1in</LeftMargin>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <CommandText>SELECT     Tbl_Dim_Store.Store_Num, Tbl_Dim_Date.Calendar_Year_ID, SUM(Tbl_Fact_Store_Inventory.On_Hand_Qty) AS SumOnHandQty, 
                      AVG(Tbl_Fact_Store_Inventory.Days_In_Stock) AS AvgDaysInStock
FROM         Tbl_Dim_Store INNER JOIN
                      Tbl_Fact_Store_Inventory ON Tbl_Dim_Store.SK_Store_ID = Tbl_Fact_Store_Inventory.SK_Store_ID INNER JOIN
                      Tbl_Dim_Date ON Tbl_Fact_Store_Inventory.SK_Date_ID = Tbl_Dim_Date.SK_Date_ID
GROUP BY Tbl_Dim_Store.Store_Num, Tbl_Dim_Date.Calendar_Year_ID
HAVING      (Tbl_Dim_Store.Store_Num = @Store_Num)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@Store_Num">
            <Value>=Parameters!Store_Num.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>DWScale</DataSourceName>
      </Query>
      <Fields>
        <Field Name="Store_Num">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Store_Num</DataField>
        </Field>
        <Field Name="Calendar_Year_ID">
          <rd:TypeName>System.Int16</rd:TypeName>
          <DataField>Calendar_Year_ID</DataField>
        </Field>
        <Field Name="SumOnHandQty">
