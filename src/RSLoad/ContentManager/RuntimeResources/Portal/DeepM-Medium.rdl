  <rd:ReportID>533fb180-4584-4845-af64-7519b299688f</rd:ReportID>
  <LeftMargin>1in</LeftMargin>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <CommandText>SELECT     Tbl_Dim_Store.Region, Tbl_Dim_Store.District, Tbl_Dim_Store.City, Tbl_Dim_Date.Calendar_Year_ID, SUM(Tbl_Fact_Store_Sales.Sales_Qty) AS Qty, 
                      Tbl_Dim_Date.Calendar_Qtr_ID, SUM(Tbl_Fact_Store_Sales.Sale_Amt) AS Amt
FROM         Tbl_Dim_Store INNER JOIN
                      Tbl_Fact_Store_Sales ON Tbl_Dim_Store.SK_Store_ID = Tbl_Fact_Store_Sales.SK_Store_ID INNER JOIN
                      Tbl_Dim_Date ON Tbl_Fact_Store_Sales.SK_Date_ID = Tbl_Dim_Date.SK_Date_ID
GROUP BY Tbl_Dim_Store.Region, Tbl_Dim_Store.District, Tbl_Dim_Store.City, Tbl_Dim_Date.Calendar_Year_ID, Tbl_Dim_Date.Calendar_Qtr_ID
HAVING      (NOT (Tbl_Dim_Store.Region IS NULL))
ORDER BY Tbl_Dim_Store.Region, Tbl_Dim_Store.District, Tbl_Dim_Store.City, Tbl_Dim_Date.Calendar_Year_ID, Tbl_Dim_Date.Calendar_Qtr_ID</CommandText>
        <DataSourceName>DWScale</DataSourceName>
      </Query>
      <Fields>
        <Field Name="Region">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>Region</DataField>
        </Field>
        <Field Name="District">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>District</DataField>
        </Field>
        <Field Name="City">
          <rd:TypeName>System.String</rd:TypeName>
          <DataField>City</DataField>
        </Field>
        <Field Name="Calendar_Year_ID">
          <rd:TypeName>System.Int16</rd:TypeName>
          <DataField>Calendar_Year_ID</DataField>
        </Field>
        <Field Name="Qty">
          <rd:TypeName>System.Int32</rd:TypeName>
          <DataField>Qty</DataField>
        </Field>
        <Field Name="Calendar_Qtr_ID">
          <rd:TypeName>System.Byte</rd:TypeName>
          <DataField>Calendar_Qtr_ID</DataField>
        </Field>
        <Field Name="Amt">
          <rd:TypeName>System.Decimal</rd:TypeName>
          <DataField>Amt</DataField>
        </Field>
      </Fields>
    </DataSet>
