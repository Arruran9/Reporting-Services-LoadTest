      <Query>
        <DataSourceName>AdventureWorks</DataSourceName>
        <CommandText>SELECT Top 1000 sh.CustomerID, sh.SalesPersonID, sh.TerritoryID, sh.PurchaseOrderNumber, sh.CurrencyCode, sh.SubTotal, sh.TaxAmt, sh.TotalDue, 
                      sh.SalesOrderNumber, sh.DueDate, sh.rowguid AS Expr1, sh.ModifiedDate AS Expr2, sh.Comment, sh.OnlineOrderFlag, sh.ContactID, 
                      sh.CreditCardExpYear, sh.CreditCardExpMonth, sh.CreditCardNumber, sh.CreditCardID, sh.ShipMethodID, sh.ShipDate, sh.ShipToAddressID, 
                      sh.BillToAddressID, sh.Status, sh.RevisionNumber, sh.OrderDate, sh.Freight, sd.LineNumber AS Expr3, sd.ProductID AS Expr4, 
                      sd.SpecialOfferID AS Expr5, sd.CarrierTrackingNumber, sd.OrderQty, sd.UnitPrice, sd.UnitPriceDiscount, sd.ModifiedDate, sd.rowguid, sd.LineTotal, 
                      Product.Name, Product.ProductNumber, Product.DiscontinuedDate, sd.SalesOrderID
FROM         SalesOrderHeader sh INNER JOIN
                      SalesOrderDetail sd ON sh.SalesOrderID = sd.SalesOrderID INNER JOIN
                      Product ON sd.ProductID = Product.ProductID
WHERE     (sd.SalesOrderID &lt; 6000)</CommandText>
      </Query>
